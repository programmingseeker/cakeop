{
	"name": "cakeopapi",
	"type": "module",
	"version": "1.0.0",
	"description": "complete backend api for an ecommerce application which sells cakes",
	"main": "server.js",
	"scripts": {
		"start": "NODE_ENV=production node backend/app",
		"server": "nodemon backend/app",
		"client": "npm start --prefix frontend",
		"dev": "concurrently \"npm run server\" \"npm run client\"",
		"data:import": "node data/seeder.js --import",
		"data:delete": "node data/seeder.js --delete"
	},
	"author": "pro developers",
	"dependencies": {
		"axios": "^0.21.0",
		"bcryptjs": "^2.4.3",
		"colors": "^1.4.0",
		"cookie-parser": "^1.4.5",
		"cors": "^2.8.5",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"jsonwebtoken": "^8.5.1",
		"mongoose": "^5.10.7",
		"morgan": "^1.10.0",
		"multer": "^1.4.2",
		"passport": "^0.4.1",
		"passport-google-oauth20": "^2.0.0",
		"react-images-upload": "^1.2.8",
		"react-multi-carousel": "^2.5.5",
		"react-router-bootstrap": "^0.25.0"
	},
	"devDependencies": {
		"concurrently": "^5.3.0",
		"nodemon": "^2.0.4"
	}
}
